{
  "collections": [
    {
      "id": "guests",
      "name": "guests",
      "type": "base",
      "schema": [
        {
          "name": "first_name",
          "type": "text",
          "required": true
        },
        {
          "name": "last_name",
          "type": "text",
          "required": true
        },
        {
          "name": "email",
          "type": "email",
          "required": true,
          "options": {
            "unique": true
          }
        },
        {
          "name": "phone",
          "type": "text",
          "required": false
        },
        {
          "name": "address",
          "type": "text",
          "required": false
        },
        {
          "name": "date_of_birth",
          "type": "date",
          "required": false
        }
      ],
      "indexes": [
        "CREATE UNIQUE INDEX idx_guests_email ON guests (email)"
      ],
      "listRule": "",
      "viewRule": "",
      "createRule": "",
      "updateRule": "",
      "deleteRule": ""
    }
  ]
}